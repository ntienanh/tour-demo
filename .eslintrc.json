{
  "plugins": ["prettier", "@tanstack/query", "@typescript-eslint", "prettier-plugin-organize-imports"],
  "parser": "@typescript-eslint/parser",
  "extends": ["next/core-web-vitals", "eslint:recommended", "prettier", "plugin:@typescript-eslint/recommended"],
  "rules": {
    "react-hooks/exhaustive-deps": 0,
    "@typescript-eslint/ban-types": 0,
    "@typescript-eslint/no-empty-interface": 0,
    "@typescript-eslint/no-explicit-any": 0,
    "@typescript-eslint/no-unused-vars": [1, { "argsIgnorePattern": "^_", "varsIgnorePattern": "^_" }],
    "@typescript-eslint/no-var-requires": true
  }
}
